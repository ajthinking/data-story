{
  "name": "@data-story/core",
  "version": "0.0.71",
  "main": "dist/index.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch:tsc": "tsc --watch",
    "tinker": "npx ts-node ./src/tinker.ts",
    "test": "yarn run -T vitest run",
    "watch:test": "yarn run -T vitest",
    "build": "tsc",
    "release": "yarn run -T release-it"
  },
  "release-it": {
    "git": false,
    "npm": {
      "publish": false,
      "skipChecks": true,
      "versionArgs": [
        "--workspaces-update=false"
      ]
    }
  },
  "dependencies": {
    "@data-story/hjson": "workspace:*",
    "@types/node": "18.14.2",
    "axios": "^1.3.4",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "4.9.5",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-shebang-plugin": "^1.1.8"
  }
}
