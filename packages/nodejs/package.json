{
  "name": "@data-story/nodejs",
  "version": "0.0.109",
  "main": "dist/index.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "bin": {
    "server": "dist/server.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch:nodejs": "tsc --watch",
    "tinker": "npx ts-node ./src/tinker.ts",
    "test": "yarn run -T vitest",
    "build": "tsc",
    "release": "yarn run -T release-it",
    "watch:server": "pm2 start ./test-server.ts --watch --no-daemon"
  },
  "release-it": {
    "git": false,
    "npm": {
      "publish": false,
      "skipChecks": true,
      "versionArgs": [
        "--workspaces-update=false"
      ]
    }
  },
  "dependencies": {
    "@data-story/core": "workspace:*",
    "axios": "^1.3.4",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "dotenv": "^16.0.3",
    "glob": "^11.0.0",
    "openai": "^3.2.1",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/ws": "^8.5.4",
    "webpack-shebang-plugin": "^1.1.8"
  }
}
